@model IEnumerable<Ticket>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Seat.Performance.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Seat.Row)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Seat.SeatNumber)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Seat.Performance.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Seat.Row)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Seat.SeatNumber)
            </td>
        </tr>
}
    </tbody>
</table>
